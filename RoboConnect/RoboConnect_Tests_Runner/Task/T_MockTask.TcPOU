    <Method Name="CyclicLogic" Id="{08e12be7-fac3-4c20-aa7e-4323956f8553}">
      <Declaration><![CDATA[METHOD PUBLIC CyclicLogic

]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE state OF
  IDLE_STATE:
    IF StateChanged() THEN
      _Busy := FALSE;
      _Done := FALSE;
      _Error := FALSE;
    END_IF
    
  BUSY_STATE:
    IF StateChanged() THEN
      _Busy := TRUE;
      _Done := FALSE;
      _Error := FALSE;
    END_IF

    IF NOT WaitForCycles(cycleCount) THEN
      RETURN;
    END_IF

    state := DONE_STATE;

  DONE_STATE:
    IF StateChanged() THEN
      _Busy := FALSE;
      _Done := TRUE;
      _Error := FALSE;
    END_IF

  ERROR_STATE:
    IF StateChanged() THEN
      _Error := TRUE;
      _Busy := FALSE;
      _Done := FALSE;
    END_IF

END_CASE]]></ST>
      </Implementation>
    </Method>
