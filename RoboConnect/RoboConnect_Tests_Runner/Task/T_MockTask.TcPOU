    <Method Name="WaitForCycles" Id="{ab9c58c6-8236-4eb0-b571-4567a7f2f18a}">
      <Declaration><![CDATA[METHOD PRIVATE WaitForCycles : BOOL
VAR_INPUT
  maxCount : UINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[counter := counter + 1;

IF counter = maxCount THEN
  counter := 0;
  WaitForCycles := TRUE;
END_IF
]]></ST>
